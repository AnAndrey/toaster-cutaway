@{
    ViewBag.Title = "Photos";
}
<div id="gallery"></div>

<script>window.jQuery || document.write('<script src="~/Scripts/jquery-1.9.1.min.js"><\/script>')</script>
<script src="~/Scripts/jquery.galereya.js"></script>
<script>
    $(function() {
        $('#gallery').galereya({
            load: function(next) {

                $.ajax({
                    url: "/api/photos",
                    method: "GET",
                    success: function(data) {
                        next(data);
                    
                    }
                });
            }
        }); 
           
    });

</script>

